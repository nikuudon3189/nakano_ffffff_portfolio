    "main": "index.js",
    "scripts": {
        "sass:watch": "sass --watch scss/style.scss:style.css scss/reset.scss:reset.css",
        "serve": "browser-sync start --server --files '*.html, *.css' --startPath index.html",
        "dev": "npm-run-all --parallel sass:watch serve",
        "build": "sass scss/style.scss:style.css --style compressed && sass scss/reset.scss:reset.css --style compressed"
    },
